<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        app:layout_constraintBottom_toTopOf="@+id/buttonPanel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonPanel"
        android:layout_width="205dp"
        android:layout_height="40dp"
        android:text="About Me"
        app:layout_constraintBottom_toTopOf="@+id/Clicky_Clicky"
        app:layout_constraintEnd_toEndOf="@+id/Clicky_Clicky"
        app:layout_constraintStart_toStartOf="@+id/Clicky_Clicky"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/Clicky_Clicky"
        android:layout_width="205dp"
        android:layout_height="40dp"
        android:text="Clicky Clicky"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/LinkCollector"
        android:layout_width="205dp"
        android:layout_height="40dp"
        android:text="Link Collector"
        app:layout_constraintEnd_toEndOf="@+id/Clicky_Clicky"
        app:layout_constraintStart_toStartOf="@+id/Clicky_Clicky"
        app:layout_constraintTop_toBottomOf="@+id/Clicky_Clicky" />

</androidx.constraintlayout.widget.ConstraintLayout>